{
    "name": "etrias/php-toolkit",
    "description": "Common PHP Tooling",
    "license": "MIT",
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "basis-company/nats": "^0.15.1",
        "monolog/monolog": "^3",
        "predis/predis": "^2.2",
        "symfony/cache": "^6",
        "symfony/console": "^7",
        "symfony/event-dispatcher": "^7",
        "symfony/http-foundation": "^7",
        "symfony/messenger": "^6",
        "symfony/property-access": "^7",
        "symfony/security-bundle": "^7",
        "symfony/serializer": "^6",
        "symfony/uid": "^7",
        "symfony/var-dumper": "^6"
    },
    "conflict": {
        "basis-company/nats": "<0.15.1"
    },
    "autoload": {
        "psr-4": {
            "Etrias\\PhpToolkit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Etrias\\PhpToolkit\\Tests\\": "tests/"
        }
    }
}

{
    "name": "etrias/php-toolkit",
    "description": "Common PHP Tooling",
    "license": "MIT",
    "require": {
        "php": "~8.1"
    },
    "require-dev": {
        "basis-company/nats": "^0.26.0",
        "doctrine/orm": "^2",
        "monolog/monolog": "^3",
        "php-http/message": "^1",
        "symfony/cache": "^6",
        "symfony/console": "^6",
        "symfony/dependency-injection": "^6",
        "symfony/doctrine-bridge": "^6",
        "symfony/doctrine-messenger": "^6",
        "symfony/event-dispatcher": "^6",
        "symfony/http-foundation": "^6",
        "symfony/lock": "^6",
        "symfony/messenger": "^6",
        "symfony/property-access": "^6",
        "symfony/security-bundle": "^6",
        "symfony/serializer": "^6",
        "symfony/stopwatch": "^6",
        "symfony/uid": "^6",
        "symfony/var-dumper": "^6"
    },
    "conflict": {
        "basis-company/nats": "<0.21.0"
    },
    "autoload": {
        "psr-4": {
            "Etrias\\PhpToolkit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Etrias\\PhpToolkit\\Tests\\": "tests/"
        }
    }
}

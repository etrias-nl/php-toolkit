{
    "name": "etrias/php-toolkit",
    "description": "Common PHP Tooling",
    "license": "MIT",
    "require": {
        "php": "^8.3"
    },
    "require-dev": {
        "basis-company/nats": "1.1.0",
        "doctrine/orm": "^2",
        "monolog/monolog": "^3",
        "php-http/client-common": "^2",
        "symfony/cache": "^7",
        "symfony/console": "^7",
        "symfony/dependency-injection": "^7",
        "symfony/doctrine-bridge": "^7",
        "symfony/doctrine-messenger": "^7",
        "symfony/event-dispatcher": "^7",
        "symfony/http-foundation": "^7",
        "symfony/lock": "^7",
        "symfony/messenger": "^7",
        "symfony/property-access": "^7",
        "symfony/security-bundle": "^7",
        "symfony/serializer": "^7",
        "symfony/stopwatch": "^7",
        "symfony/uid": "^7",
        "symfony/var-dumper": "^7"
    },
    "autoload": {
        "psr-4": {
            "Etrias\\PhpToolkit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Etrias\\PhpToolkit\\Tests\\": "tests/"
        }
    }
}

{
    "name": "etrias/php-toolkit",
    "description": "Common PHP Tooling",
    "license": "MIT",
    "require": {
        "php": "^8.3"
    },
    "require-dev": {
        "basis-company/nats": "^1",
        "doctrine/migrations": "^3",
        "doctrine/orm": "^2",
        "guzzlehttp/guzzle": "^7",
        "monolog/monolog": "^3",
        "php-http/client-common": "^2",
        "php-soap/ext-soap-engine": "^1",
        "phpro/soap-client": "^4",
        "symfony/cache": "^8",
        "symfony/console": "^8",
        "symfony/dependency-injection": "^8",
        "symfony/doctrine-bridge": "^8",
        "symfony/doctrine-messenger": "^8",
        "symfony/event-dispatcher": "^8",
        "symfony/http-foundation": "^8",
        "symfony/lock": "^8",
        "symfony/messenger": "^8",
        "symfony/property-access": "^8",
        "symfony/security-bundle": "^8",
        "symfony/serializer": "^8",
        "symfony/stopwatch": "^8",
        "symfony/uid": "^8",
        "symfony/var-dumper": "^8"
    },
    "autoload": {
        "psr-4": {
            "Etrias\\PhpToolkit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Etrias\\PhpToolkit\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": false
        }
    }
}
